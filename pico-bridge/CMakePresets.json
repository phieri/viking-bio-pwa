{
  "version": 2,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 19,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "pico-default",
      "hidden": false,
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "PICO_SDK_PATH": "${env:PICO_SDK_PATH}",
        "PICO_BOARD": "pico_w"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "default",
      "configurePreset": "pico-default"
    }
  ]
}
